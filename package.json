  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" --config ./.prettierrc",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "node dist/seeder",
    "seed:migrate": "node dist/seeder-migrate",
    "seed:refresh": "node dist/seeder --refresh",
    "postinstall": "npx husky install",
    "typeorm": "npx typeorm-ts-node-commonjs -d src/database/dataSource/data-source.ts",
    "typeorm:migrate": "yarn typeorm migration:run",
    "typeorm:create": "npx typeorm migration:create src/database/migrations/fileName -t",
    "typeorm:revert": "yarn typeorm migration:revert",
    "command": "TS_NODE_PROJECT=./tsconfig.json ts-node -r tsconfig-paths/register ./src/main-cli.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^7.5.0",
