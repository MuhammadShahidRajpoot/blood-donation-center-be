    "moment": "^2.29.4",
    "moment-timezone": "^0.5.45",
    "multer": "^1.4.5-lts.1",
    "nest-commander": "^3.12.5",
    "nestjs-seeder": "^0.3.1",
    "passport": "^0.6.0",
