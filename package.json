    "@nestjs/platform-socket.io": "^10.3.7",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^10.3.7",
    "@sentry/node": "^7.56.0",
    "@sentry/tracing": "^7.56.0",
    "@types/bcrypt": "^5.0.0",
    "@types/lodash": "^4.14.196",
    "aws-sdk": "^2.1471.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
